# Production env vars for AWS deployment. Real values should come from Secrets Manager or SSM at runtime.
RAPIDAPI_KEY=__SET_RAPIDAPI_KEY_FROM_SECRET__
PORT=4002
# ElastiCache Redis endpoint (auth token optional depending on configuration)
REDIS_ADDR=prod-search-cache.abc123.ng.0001.use1.cache.amazonaws.com:6379
REDIS_PASSWORD=__SET_REDIS_AUTH_TOKEN_IF_ENABLED__
REDIS_DB=0
# RDS Postgres connection string; require SSL in production
PG_DSN=postgres://search_api:__SET_DB_PASSWORD_FROM_SECRET__@prod-search-db.abc123xyz.us-east-1.rds.amazonaws.com:5432/realestate?sslmode=require&search_path=ingest,public
ENABLE_INDEXER=1
